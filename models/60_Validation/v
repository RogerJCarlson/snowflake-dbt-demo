SELECT CDM_QRY_PROCEDURE_OCCURRENCE.PROCEDURE_OCCURRENCE_ID, CDM_QRY_PROCEDURE_OCCURRENCE.PERSON_ID, CDM_QRY_PROCEDURE_OCCURRENCE.PROCEDURE, CDM_QRY_PROCEDURE_OCCURRENCE.PROCEDURE_DATETIME, CDM_QRY_PROCEDURE_OCCURRENCE.[procedure type], CDM_QRY_PROCEDURE_OCCURRENCE.MODIFIER, CDM_QRY_PROCEDURE_OCCURRENCE.QUANTITY, CDM_QRY_PROCEDURE_OCCURRENCE.PROVIDER_NAME, CDM_QRY_PROCEDURE_OCCURRENCE.VISIT_OCCURRENCE_ID, CDM_QRY_PROCEDURE_OCCURRENCE.PROCEDURE_SOURCE_VALUE, CDM_QRY_PROCEDURE_OCCURRENCE.PROCEDURE_SOURCE, CDM_QRY_PROCEDURE_OCCURRENCE.QUALIFIER_SOURCE_VALUE, CDM_QRY_PROCEDURE_OCCURRENCE.SDT_TAB, CDM_QRY_PROCEDURE_OCCURRENCE.NK, CDM_QRY_PROCEDURE_OCCURRENCE.ETL_MODULE, CDM_QRY_PROCEDURE_OCCURRENCE.VISIT_SOURCE_VALUE
FROM CDM_QRY_PROCEDURE_OCCURRENCE
WHERE (((CDM_QRY_PROCEDURE_OCCURRENCE.PERSON_ID)=[forms]![participants]![pid]));
